{
  "manifest_version": 3,
  "name": "Civitai Auto Expand Prompts",
  "version": "1.0.0",
  "description": "Auto-expands 'Show more' in the Prompt panel on Civitai.",
  "icons": {
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://civitai.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
